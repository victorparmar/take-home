{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "e2e",
        "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
        "args": [
          "-u",
          "tdd",
          "--timeout",
